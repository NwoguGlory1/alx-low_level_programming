#include <stdio.h>
#include <stdlib.h>

void save_password() {
	char password[] = "Congratulations!"

	FILE *file;
	file = fopen("101-password", "w");

	if (file == NULL)
{
	printf("Failed to open the file.\n");
	return;
}

	fprintf(file, "%s", password);
	fclose(file);

	printf("Password saved to 101-password\n");
}

int main()
{
	save_password();
	return (0);
}
